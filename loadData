#Loads all lessons into redis
#
# Lesson 1
../redis-2.0.0-rc4/redis-cli set lesson:1:name Names
FILE=`cat data/1.html`
../redis-2.0.0-rc4/redis-cli set lesson:1:intro "$FILE"
FILE=`cat data/1-1q.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:q "$FILE"
FILE=`cat data/1-1a.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:a "$FILE"
FILE=`cat data/1-2q.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:q "$FILE"
FILE=`cat data/1-2a.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:a "$FILE"
FILE=`cat data/1-3q.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:q "$FILE"
FILE=`cat data/1-3a.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:a "$FILE"
FILE=`cat data/1-4q.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:q "$FILE"
FILE=`cat data/1-4a.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:a "$FILE"
FILE=`cat data/1-5q.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:q "$FILE"
FILE=`cat data/1-5a.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:a "$FILE"
FILE=`cat data/1-6q.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:q "$FILE"
FILE=`cat data/1-6a.html`
../redis-2.0.0-rc4/redis-cli rpush lesson:1:a "$FILE"
